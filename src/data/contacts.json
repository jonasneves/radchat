    },
    {
      "condition": "after_hours AND (ct OR mri OR nuclear_medicine)",
      "action": "contact_senior_resident",
      "phone": "681-4422",
      "rationale": "After-hours CT, MRI, and Nuclear Medicine questions go to senior resident"
    },
    {
      "condition": "weekend AND any_imaging_question",
      "action": "use_weekend_schedule",
      "rationale": "Weekend hours are 7:30am-12pm and 12pm-7:30am with different on-call schedules"
    }
  ]
